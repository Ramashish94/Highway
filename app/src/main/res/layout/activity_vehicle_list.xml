<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".customer.customerActivity.VehicleListActivity">

    <LinearLayout
        android:id="@+id/Llayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:minWidth="@dimen/margin_100"
        android:orientation="horizontal"
        android:padding="@dimen/margin_5">

        <LinearLayout
            android:id="@+id/Llayout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:minWidth="100dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tataAceTv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tata Ace"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <ImageView
                android:id="@+id/vehicleImg1"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle1_tata_ace_icon1_un_selected"
                />

            <ImageView
                android:id="@+id/vehicleImg2"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle2_champion_icon2_un_selected"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/vehicleImg3"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle3_pickup_icon3__un_selected"
                android:visibility="gone"/>

            <ImageView
                android:id="@+id/vehicleImg4"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle4_evehicle_icon4_un_selected"
                android:visibility="gone"/>

            <ImageView
                android:id="@+id/vehicleImg5"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle5_echo_icon5_un_selected"
                android:visibility="gone"/>

            <ImageView
                android:id="@+id/vehicleImg6"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle6_icon6_un_selected"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/vehicleImg1Selected"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle11_tata_ace_icon11_selected"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/vehicleImg2Selected"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle22_champion_icon22_selected"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/vehicleImg3Selected"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle33_pickup_icon33_selected"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/vehicleImg4Selected"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle44_evehicle_icon44_selected"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/vehicleImg5Selected"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle55_echo_icon55_selected"
                android:visibility="gone"/>
            <ImageView
                android:id="@+id/vehicleImg6Selected"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/black"
                android:src="@drawable/vehicle66_icon66_selected"
                android:visibility="gone"/>

            <TextView
                android:id="@+id/timeDurationTv2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="8 mins"
                android:textColor="@color/black"
                android:textSize="10sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/faireChargeTv3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="@string/Rs_one1"
                    android:textColor="@color/black"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/infoImg"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="2.5sp"
                    android:src="@drawable/info_icon" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


</RelativeLayout>